        $('#g-navi a').click(function () {
          var elmHash = $(this).attr('href');
          var pos = Math.round($(elmHash).offset().top-120);
          $('body,html').animate({scrollTop: pos}, 500);
          return false;
        });
        $(window).scroll(function () {
          FixedAnime();
        });
        $(window).on('load', function () {
          FixedAnime();
        });
